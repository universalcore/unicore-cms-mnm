<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      metal:define-macro="layout"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="unicorecmsmnm"
      dir="${view.get_language_direction()}">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link rel="shortcut icon" type="image/x-icon" href="{{ STATIC_URL }}img/favicon.ico" />
    <title>Malaria No More / Universal Core</title>
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=0;" />
    <meta name="description" content=""/>
    <meta name="keywords" content="malaria, health"/>
    <link href="${request.static_url('cms:static/css/style.css')}?v=20150203" rel="stylesheet" type="text/css" />
</head>
<body>
<div id="wrap">
    <div class="lang" tal:condition="len(view.get_display_languages())>1">
      <span tal:repeat="item view.get_display_languages()">
        <a tal:attributes="{'class':'active'} if view.locale==item[0] else {}" href="/locale/${item[0]}/">${item[1]}</a> |
      </span>
      <a href="/locale/change/" tal:condition="len(view.get_available_languages)>2" class="lang-change">&hellip;</a>
    </div>

    <div id="header">
        <a href="/"><img src="${request.static_url('cms:static/img/logo.png')}?v=20141014" border="0" /></a>
    </div>

    <div id="content">

        <div metal:define-slot="content">
        </div>




    </div>
   <div metal:define-slot="search" metal:use-macro="view.search_box_template"></div>
    <div id="footer">
        <span>&copy; Malaria No More 2014</span>
    </div>

</div>
</body>


</html>
